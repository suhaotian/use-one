{
  "name": "use-one",
  "version": "1.2.0-beta.0",
  "license": "MIT",
  "packageManager": "pnpm@7.28.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suhaotian/use-one.git"
  },
  "author": {
    "name": "@suhaotian"
  },
  "bugs": {
    "url": "https://github.com/suhaotian/use-one/issues"
  },
  "homepage": "https://github.com/suhaotian/use-one",
  "keywords": [
    "state",
    "management",
    "manager",
    "store",
    "react",
    "reactjs",
    "hooks",
    "react-hooks",
    "typescript"
  ],
  "react-server": "./lib/create.server.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "react-server": "./lib/create.server.js",
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "module": "./dist/index.esm.js"
  },
  "scripts": {
    "build": "bunchee ./src/index.ts && npm run build:lib",
    "build:lib": "rm -rf lib && tsc --project tsconfig.build.json",
    "start:example": "cd example && pnpm i && pnpm start",
    "serve:example": "cd example && pnpm i && pnpm serve",
    "test": "pnpm jest",
    "test:e2e": "playwright test e2e-test",
    "start-publish": "npm run build && npm publish --access public --no-git-checks --provenance --registry=https://registry.npmjs.org"
  },
  "files": [
    "README.md",
    "dist",
    "lib/create.server.d.ts",
    "lib/create.server.js",
    "lib/types.d.ts"
  ],
  "dependencies": {
    "eventemitter3": "^5.0.0",
    "server-only": "^0.0.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "bunchee": "^3.3.4",
    "react-test-renderer": "^18.2.0",
    "babel-jest": "^29.5.0",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@playwright/test": "^1.33.0",
    "typescript": "^5.1.3",
    "tslib": "^2.5.3",
    "immer": "^10.0.3",
    "@tsconfig/recommended": "^1.0.3"
  }
}
